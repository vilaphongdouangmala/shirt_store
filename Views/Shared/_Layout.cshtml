<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!--site css-->
    <link rel="stylesheet" href="~/css/site.css" />

    <!--Quasar-->
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,500,700,900|Material+Icons" rel="stylesheet" type="text/css">
    <link href="https://maxst.icons8.com/vue-static/landings/line-awesome/line-awesome/1.3.0/css/line-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="https://cdn.jsdelivr.net/npm/quasar@2.11.2/dist/quasar.css" rel="stylesheet" type="text/css">
 

</head>
<body>
    <div id="app">

        <q-layout class="shadow-2 rounded-borders">
            <q-header class="bg-black" elevated>
                <q-toolbar>
                    <q-toolbar-title class="text-center text-bold">Chic Streetwear</q-toolbar-title>
                </q-toolbar>

                <q-tabs v-model="tab">
                <q-tab name="images" label="Orders" onclick="window.location='/order/'"></q-tab>
                <q-tab name="videos" label="SKUs" onclick="window.location='/sku/'"></q-tab>
                <q-tab name="articles" label="Products" onclick="window.location='/product/'"></q-tab>
                </q-tabs>
            </q-header>
            <q-page-container>
                <q-page class="q-pa-md">
                    @RenderBody()
                </q-page>
            </q-page-container>
        </q-layout>
    </div>

   
    <!--site js-->
    <script src="~/js/site.js" asp-append-version="true"></script>


    <!--quasar cdn-->
    <!-- Add the following at the end of your body tag -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,500,700,900|Material+Icons" rel="stylesheet" type="text/css">
    <link href="https://maxst.icons8.com/vue-static/landings/line-awesome/line-awesome/1.3.0/css/line-awesome.min.css" rel="stylesheet" type="text/css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.27.0/moment.min.js"></script>
    <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/vue@3/dist/vue.global.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/quasar@2.11.2/dist/quasar.umd.js"></script>
 
    @RenderSection("code", required: false)
</body>
</html>
